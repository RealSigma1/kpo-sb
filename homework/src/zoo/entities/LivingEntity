package com.zoo.entities;

import com.zoo.interfaces.IAlive;

public abstract class LivingEntity implements IAlive {
    protected String name;
    protected int food;

    public LivingEntity(String name, int food) {
        this.name = name;
        this.food = food;
    }

    @Override
    public String getName() {
        return name;
    }

    @Override
    public int getFood() {
        return food;
    }
}